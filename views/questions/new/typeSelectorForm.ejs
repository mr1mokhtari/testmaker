	<div class=" delay_hide_sceditor mc_formdiv" style="display: block;">
		<div class="multiple_choice_answers">
		<h5>Answers <span class="showTooltip tooltip" title="Type at least two answer options and select which answer/s are correct.">Tip</span></h5>
			<%var mc_options=["A","B","C","D","E","F","G","H","I","J"]%>
			<%mc_options.forEach((op,o)=>{%>
			<%if(o==4){%>
				<div id="tog_extra" class="unit_box col_left hideme"><a href="#" class="btn btn-add-wysiwyg hideme">Add more answer options</a></div>
				<div class="hide delay_hide_sceditor" id="dotog_extra" style="display: none;">
			<%}%>
			<div class="unit_box">
				<div class="answer">
					<div class="answer_heading">
						<h5 class="">(<%=op%>)</h5>
						<input class="showCorrect mc_forminput" type="checkbox" name="question[mc_correct]" value="<%=o+1%>" id="a<%=o+1%>">
						<label for="a<%=o+1%>" class="">This answer option is correct</label>
					</div>
					<textarea name="question[mc_options]" rows="3" cols="67" id="ans<%=o+1%>" class="qclear mc_forminput fullBBcodeEditor" style="display: none;"><%if(question!=0){%><%=question.mc_options[o].answer%><%}%></textarea>
