				<input type="text" name="positive_score" id="positive_score" class="m_forminput" size="3" value="1" disabled="disabled">&nbsp;<i>pts</i>&nbsp;&nbsp;&nbsp;&nbsp;
				<span class="matching_minus_symbol">−</span>
				<input type="text" name="negative_score" id="negative_score" class="m_forminput" size="3" value="0" disabled="disabled">&nbsp;<i>pts</i>&nbsp;&nbsp;
				<a href="#" class="btn btn-white" id="set_all_points">Set all points</a>
	  		</div>
			<%var m_options=["A","B","C","D","E","F","G","H","I","J"]%>
			<%m_options.forEach((mop,mn)=>{%>
				<%if(mn==5){%>
					<div id="tog_extra_matching" class="unit_box unit_box_matching col_center">
						<div id="loading" style="display:none">
							<img src="/img3/icon_loading_circleV3.gif" alt="Please wait">
						</div>
						<a href="#" class="btn btn-add-wysiwyg" id="add_more_matching">Add more matching pairs</a>
					</div>
					<div class="hide delay_hide_sceditor" id="dotog_extra_matching" style="display: none;">
				<%}%>
				<div class="unit_box unit_box_matching">
					<div class="matching_pairs">
						<div class="matching_row">
							<div class="matching_col col_left">
								<div>
									<div class="grading_style_advance hide" style="display: none;">
										<span class="matching_plus_symbol">+</span>&nbsp;
										<input type="text" name="question[p_score]" class="m_forminput" size="3" id="p_score_<%=mn+1%>" value="1" disabled="disabled"> <i>pts</i>&nbsp;&nbsp;&nbsp;&nbsp;
										<span class="matching_minus_symbol">−</span>&nbsp;
										<input type="text" name="question[n_score]" class="m_forminput" size="3" id="n_score_<%=mn+1%>" value="0" disabled="disabled"> <i>pts</i>
									</div>
								</div>
							</div>
						</div>
						<div class="matching_row">
							<div class="matching_col">
								<div class="matching_col_header">Clue <%=mop%></div>									<textarea name="question[matching_query]" rows="3" cols="67" id="matching_query_<%=mn+1%>" class="qclear m_forminput fullBBcodeEditor" style="display: none;" disabled="disabled"></textarea>
								<div class="sceditor-container wysiwygMode ltr" style="width: 400px;" id="editor_matching_query_<%=mn+1%>">							
									<textarea style="display: none; width: 390px; height: 93px;" dir="ltr" disabled="disabled"></textarea>
									<div class="sceditor-grip"></div>
									<div class="sceditor-resize-cover" style="display: none;"></div>
								</div>
							</div>
							<div class="matching_col col_middle">
								<img class="small_icon" src="/img3/icon-breadcrumb-arrow.png" srcset="/img3/icon-breadcrumb-arrow_3x.png 2x" width="10" height="7">
							</div>
							<div class="matching_col">
								<%if(mn==0){%>
								<div class="matching_col_header col_margin_left">
									Match <%=mop%>
								</div>
								<%}%>
								<div class="<%if(mn==0){%>matching_answer_tabs<%}else{%>matching_answer_tabs<%=mn+1%><%}%> col_margin_left" id="matching_answer_tabs">
									<%if(mn!=0){%>
<div id="new_answer_<%=mn+1%>">Match <%=mop%><span class="matching_header">
						<select class="qclear matching_dropdown" name="question[matching_dropdown]" id="matching_dropdown_<%=mn+1%>">
							<option value="blank">Type New Match</option>
							<!-- <option value="matching_row_1">Same as Match A</option> -->
						</select>
					</span></div>
									<%}%>
									<div class="matching_answer_input<%if(mn!=0){%>_<%=mn+1%><%}%>">
										<input type="text" id="matching_input_<%=mn+1%>" class="qclear matching_input" value="" maxlength="30" disabled="disabled" name='question[matching_input]'>
										<textarea rows="3" cols="49" id="matching_option_<%=mn+1%>" class="qclear hide m_forminput fullBBcodeEditor" style="display: none;" disabled="disabled" name='question[matching_option]'></textarea>
										<div class="sceditor-container wysiwygMode ltr" style="width: 400px; display: none;" id="editor_matching_option_<%=mn+1%>">										 
											<textarea style="display: none; width: 390px; height: 93px;" dir="ltr" disabled="disabled"></textarea>
											<div class="sceditor-grip"></div>
											<div class="sceditor-resize-cover" style="display: none;"></div>
										</div>
										<input type="hidden" name="matching_result_<%=mn+1%>" id="matching_result_<%=mn+1%>" disabled="disabled">
									</div>
									<div class="bbcode matching-imgw matching_answer_existing_<%=mn+1%>"></div>
								</div>
							</div>
						</div>
					</div>
				</div>
				<%if(mn==9){%>
					</div>
				<%}%>
			<%})%>
			
			<div class="dummy_answer_box">
				<div id="tog_dummy_answer" class="unit_box unit_box_matching col_center hideme">
					<a href="#" class="btn btn-add-wysiwyg hideme">Add more incorrect matches</a>
				</div>
				<div class="hide delay_hide_sceditor col_left" id="dotog_dummy_answer" style="display: none;">
				<%var inm_options=["A","B","C","D","E"]%>
					<%inm_options.forEach((imop,imn)=>{%>
					<div class="unit_box unit_box_matching extra_content">
						<div class="matching_col_header">
							Incorrect Match <%=imop%> 
							<span class="showTooltip tooltip" title="Incorrect matches can raise the question difficulty because Test takers cannot guess the last unselected match or make assumptions when Clues and Matches are equal.">Tip</span>
						</div>
						<div class="matching_answer_input">
							<input type="text" name="question[matching_incorrect_input]" id="matching_incorrect_input_<%=imn%>" class="qclear matching_input" value="" maxlength="30" disabled="disabled">
							<textarea rows="3" cols="67" name="question[matching_incorrect_option]" id="matching_incorrect_option_<%=imn%>" class="qclear m_forminput fullBBcodeEditor " style="display: none;" disabled="disabled"></textarea>
							<div class="sceditor-container wysiwygMode ltr" style="width: 400px; display: none;" id="editor_matching_incorrect_option_<%=imn%>">
								 
								<textarea style="display: none; width: 390px; height: 93px;" dir="ltr" disabled="disabled"></textarea>
								<div class="sceditor-grip"></div>
								<div class="sceditor-resize-cover" style="display: none;"></div>
							</div>
							<input type="hidden" name="matching_dummy_<%=imn%>" id="matching_dummy_<%=imn%>" disabled="disabled">
						</div>
					</div>
					<%})%>
				</div>
			</div>
			<div class="clearheight"></div>
		</div>
	</div>
	<%}%>
	<!-- Feedback -->
	
	<div class="delay_hide_sceditor hide e_formdiv mc_formdiv tf_formdiv ft_formdiv p_formdiv m_formdiv" style="display: block;">
		<h5>Give Feedback</h5>
		<div class="unit_box">
			<a href="#" id="tog_feedback" class="btn add-feedback hideme">Add question feedback</a>
			<div class="hide delay_hide_sceditor" id="dotog_feedback" style="display: none;"><div>
				<h5 class="green">Feedback: Correctly answered
		<span class="showTooltip tooltip" title="Feedback is optional. This text will show when Answer is correct. Adding rich media and Links is allowed just like Question and Answer Text.">Tip</span></h5>
			<textarea class="edit_qboxes question_text qclear mc_forminput tf_forminput ft_forminput e_forminput p_forminput fullBBcodeEditor hides" name="question[correct_feedback]" rows="2" cols="67" id="corect_feedback"><%if(question!=0){%><%=question.correct_feedback%><%}%> </textarea><br>
				<div class="sceditor-container wysiwygMode ltr" style="width: 540px;" id="editor_correct_feedback"></div>
			<!-- <a href="#" class="upgradeRpc explain-underline">How to add feedback</a> -->
				</div>
				<div class="clearheight"></div>
				<div>
					<div class="incorrect_feedback_div">
						<h5 class="red">Feedback: Incorrectly answered 
						<span class="showTooltip tooltip" title="Feedback is optional. This text will show when Answer is incorrect. Adding rich media and Links is allowed just like Question and Answer Text.">Tip</span></h5>
					</div>
					<textarea class="edit_qboxes question_text qclear mc_forminput tf_forminput ft_forminput e_forminput p_forminput fullBBcodeEditor hides" name="question[incorrect_feedback]" rows="2" cols="67" id="inccorect_feedback"><%if(question!=0){%><%=question.incorrect_feedback%><%}%></textarea><br>
					<div class="sceditor-container wysiwygMode ltr" style="width: 540px;" id="editor_incorrect_feedback">	</div>
		<!-- <a href="#" class="upgradeRpc explain-underline">How to add feedback</a> -->
				</div>
			</div>
		</div>
		<div class="clearheight"></div>
	</div>
<!-- Category / Points / Shuffle Point -->
<div class="delay_hide_sceditor hide e_formdiv mc_formdiv tf_formdiv ft_formdiv p_formdiv m_formdiv" style="display: block;">
	<h5>Settings</h5>
	<div class="unit_box">
		<h5>
			Category 
			<span class="showTooltip tooltip" title="Select the category for this question.">Tip</span>
		</h5>
		<select name="cat_id" id="cat_id" class="no_parent_cat_selection parent-cat-dd">
	<%pcats.forEach(function(pcat,i){%>
		<option value="pc<%=pcat._id%>" disabled="">- <%=pcat.parent_cat_name%> -</option>
		<%pcat.cat_name.forEach(function(subcat,j){%>
			<%scats.forEach(function(scat,k){%>
				<%if(JSON.stringify(scat._id)==JSON.stringify(subcat)){%>
					<option value="p<%=pcat._id%>sc<%=scat._id%>" <%if(question!=0){if(question.sub_cat_id==scat._id){%>selected<%}}%>>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%=scat.cat_name%></option>
				<%}%>
			<%})%>
		<%})%>
	<%})%>
			<!-- <option value="p0" disabled="">- Generic Parent -</option>
			<option value="0" selected="selected">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Generic</option> -->
		</select> 
		<a href="#" id="createCatRpc_" class="not-ie67 not-ie8 littlelink">
			New Category
		</a>
		<span class="showTooltip tooltip" title="Categorizing your Tests and Questions allows you to easily keep these items organized. You can also view results by Category to help better analyze problem areas.">Tip</span>
		<div id="show_point_available">
			<hr class="line_divider">
			<h5>Points Available</h5>
			<span class="showTooltip tooltip" title="How many points this question is worth. Decimals are allowed. EG: 1 or 2.5">Tip</span>
				<input type="text" id="points" name="question[points]" maxlength="5" <%if(question!=0){%>value="<%=question.points%>"<%}else{%>value="1"<%}%> size="2">
				<span id="calculated_points" style="display: none;"></span> 
				<a class="points_recalculate " style="display: none;">
					Calculate
				</a>
				<div id="warn_change_points"></div>
			</div>
		<div class="hide m_formdiv" style="display: none;">
			<hr class="line_divider">
			<div id="show-shuffle-mode">
				<h5>Shuffle Mode</h5>
				<input type="radio" name="shuffle_m" checked="checked" value="0" id="sh_1" class="m_forminput"> 
				<label for="sh_1">
					Shuffle Matches Only 
					<span class="showTooltip tooltip" title="Shuffle the match options on the right side and keep the clues on the left side in order">Tip</span>
				</label>
				<div class="clearheight5"></div>
				<input type="radio" name="shuffle_m" value="1" id="sh_2" class="m_forminput"> 
				<label for="sh_2">
					Shuffle Clues Only 
					<span class="showTooltip tooltip" title="Shuffle the clue options on the left side and keep the matches on the right side in order">
						Tip
					</span>
				</label>
				<div class="clearheight5"></div>
				<input type="radio" name="shuffle_m" value="2" id="sh_3" class="m_forminput"> 
				<label for="sh_3">
					Shuffle Clues and Matches 
					<span class="showTooltip tooltip" title="Shuffle both clue and match options">Tip</span>
				</label>
			</div>
		</div>
		<div class="hide mc_formdiv" style="display: block;">
			<hr class="line_divider">
			<div id="random_options">
				<h5>Randomize Answers
		 			<span class="showTooltip tooltip" title="Show answer options in random order.">Tip</span>
				</h5>
				<input type="radio" name="question[random_a]"  value="0" id="ra_1" class="mc_forminput"  <%if(question!=0){if(question.random_a=='0'){%>checked="checked"<%}}%> <%if(question==0){%>checked="checked"<%}%> > 
				<label for="ra_1">No</label>
				<div class="clearheight5"></div>
				<input type="radio" name="question[random_a]" value="1" id="ra_2" class="mc_forminput" <%if(question!=0){if(question.random_a=='1'){%>checked="checked"<%}}%> > 
				<label for="ra_2">Yes</label>
			</div>
		</div>
		<div class="hide mc_formdiv" style="display: block;">
			<div id="show_radio_or_checkbox">
				<hr class="line_divider">
				<h5>Answer Selection</h5>
				<input type="radio" name="question[radio_or_checkbox_not_used]" <%if(question!=0){if(question.radio_or_checkbox_not_used=='0'){%>checked="checked"<%}}%> <%if(question==0){%>checked="checked"<%}%> value="0" id="roc_0" class="mc_forminput"> 
				<label for="roc_0">
					Radio buttons 
					<span class="explain">
						- Only one answer option can be selected
					</span>
				</label>
				<span class="showTooltip tooltip" title="Radio buttons are used to enforce (and indicate) only one answer can be selected as the correct answer.">Tip</span>
				
				<div class="clearheight5"></div>
				<input type="radio" name="question[radio_or_checkbox_not_used]" value="1" <%if(question!=0){if(question.radio_or_checkbox_not_used=='1'){%>checked="checked"<%}}%> id="roc_1" class="mc_forminput"> 
				<label for="roc_1">
					Checkboxes 
					<span class="explain">
						- Multiple answer options can be selected
					</span>
				</label>
			<span class="showTooltipHTML tooltip" title="Checkboxes are used to indicate one or more answers can be selected as the correct answer (Tip: Checkboxes can make questions more difficult when only one answer is correct).">Tip</span>
			</div>
		</div>
		<div class="hide mc_formdiv m_formdiv" style="display: block;">
			<div id="show_grade_style" style="display: none;">
				<hr class="line_divider">
				<h5>Grading Scale</h5>
				<input type="radio" name="question[grade_style]" <%if(question!=0){if(question.grade_style=='0'){%>checked="checked"<%}}%> <%if(question==0){%>checked="checked"<%}%> value="0" id="gs_0" class="mc_forminput"> 
				<label for="gs_0">Off</label>
				<span class="showTooltipHTML tooltip" title="Full points for correct answer<br/>Zero points if any error is made">Tip</span>
				<div class="clearheight5"></div>
				<input type="radio" name="question[grade_style]" value="1" id="gs_1" <%if(question!=0){if(question.grade_style=='1'){%>checked="checked"<%}}%> class="mc_forminput"> 
				<label for="gs_1">Partial with deduction</label>
				<span class="showTooltip tooltip" title="Partial points for some correct, remove partial points for incorrect answers">Tip</span>
				<div class="clearheight5"></div>
				<input type="radio" name="question[grade_style]" value="2" id="gs_2" class="mc_forminput" <%if(question!=0){if(question.grade_style=='2'){%>checked="checked"<%}}%>> 
				<label for="gs_2">Partial without deduction</label>
				<span class="showTooltip tooltip" title="Partial points for some correct, do not remove points for incorrect answers">Tip</span>
			</div>
		</div>
	</div>
	</div>
	
<!-- ???	 -->
<div id="div_preview" class="hide editor bbcode" style="display: none;"></div>
