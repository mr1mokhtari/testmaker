<!-- Test Base Template START -->
<script type="text/template" id="testBaseTemplate">
	<div class="innerWrapper">
		<div class="col-span-7">
			<p class="breadcrumb3">
				<span class="first">Tests</span>
			</p>
		</div>
		<div class="col-span-3"><ul class="menu-sections" id="walkthrough-menu">
			<li class="test current"><a href="/tests/#tests"><span>Tests</span></a></li>
			<li class="group"><a class="sect" href="/tests/groups"><span>Groups</span></a></li>
			<li class="link"><a class="sect" href="/tests/links"><span>Links</span></a></li></ul>
		</div>
		<div class="clearheight20"></div>
		<div class="clearheight20"></div>
		<ul class="cmtableTabs">
			<li class="tabon"><a href="/a/tests/#tests">Tests</a></li>
			<li class="taboff"><a href="/a/tests/qb/?all=1">Question Bank</a></li>
			<li class="taboff"><a href="/a/tests/categories/">Categories</a></li>
			<li class="taboff"><a href="/a/tests/files/">Files</a></li>
			<li class="taboff"><a href="/a/tests/certificates/">Certificates</a></li>
			<li class="taboff"><a href="/a/tests/community/">Community</a></li>
		</ul>
		<div class="table">
			<div class="table-utility row ">
				<div class="col-span-1">
					<div class="dropdown dropdown-x hide_if_no_data">
						<a class="dropdown-toggle no_anchor" href="#"> Options </a>
						<ul class="dropdown-menu">
							<li><a href="#" class="expandAll">Expand All</a></li>
							<li><a href="#" class="collapseAll">Collapse All</a></li>
						</ul>
					</div>
				</div>
				<div class="col-span-7">
					<span class="hide_if_no_data not-ie67">
						<span class="hide_if_no_data search_filter">
							<input type="text" id="test_search" placeholder="Search Test Name" class="hide_if_no_data search_filter"  onClick="this.select();"/>
						</span>
					</span>
					<span id="cat_filter" class="hide_if_no_data"></span>
					<span class="hide_if_no_data not-ie67 dropdown search_filter">
						<a href="#" class="littledark"><i class="fa fa-filter" aria-hidden="true"></i> Sort</a>
						<ul class="dropdown-menu filter">
							<li><a class="sortAlphabetical" href="#">Alphabetical</a></li>
		                    <li><a class="sortDateAdded" href="#">Recently Added</a></li>
						</ul>
					</span>
				</div>
				<div class="col-span-2 col-btns">
					<a class="btn btn-create" href="/a/tests/test/new/"> New Test <span class="caret">&nbsp;</span></a>
				</div>
			</div>
			<!-- TESTS LIST: START -->
			<ul class="table-body"></ul>
			<!-- TESTS LIST: END -->

			<div class="row col_center hide" id="load_more_row"><a href="#" class="loadMore btn ">Load More Tests</a></div>
			<div class="table-utility row bellow">
				<div class="col-span-8">
					<div class="dropdown dropdown-x hide_if_no_data_bottom">
						<a class="dropdown-toggle no_anchor" href="#"> Options </a>
						<ul class="dropdown-menu">
							<li><a href="#" class="expandAll">Expand All</a></li>
							<li><a href="#" class="collapseAll">Collapse All</a></li>
						</ul>
					</div>
				</div>
				<div class="col-span-2 col-btns hide_if_no_data_bottom">
					<a class="btn btn-create" href="/a/tests/test/new/"> New Test <span class="caret">&nbsp;</span></a>
				</div>
			</div>
		</div>
</script>
<!-- Test Base Template END -->
<!-- Test Empty Template : START -->
