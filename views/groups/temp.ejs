	<li>
		<div class="row single-hover-row">
			<div class="col-span-10">
				<p class="test-name name">You have not created any Tests yet. Learn about Tests in the <a href="/a/help/">Help Section</a>.</p>
			</div>
		</div>
	</li>
</script>
<!-- Test Empty Template END -->
<!-- Test Template : START -->
<script type="text/template" id="testTemplate">
	<div class="row accordion-toggle scrollTop closed test_container">
		<div class="col-span-6">
			<h4 class="test-name name">{{=T.test_name}}</h4>
		</div>
		<div class="col-span-4 col-btns">
			<a href="/tests/test/?test_id={{=T._id}}" class="edit-test">Edit</a>
			<a href="/assign/groups/?test_id={{=T._id}}" class="assign-test">Assign</a>
			<div class="dropdown">
				<a class="view-analytics no_anchor" href="#">Statistics <span class="caret">&nbsp;</span></a>
				<ul class="dropdown-menu">
					<li><a class="" href="/results/tests/test/?test_id={{=T._id}}">By Group</a></li>
					<li><a class="" href="/results/questions/?test_id={{=T._id}}">By Question</a></li>
				</ul>
			</div>
		</div>
		<span class="caret">&nbsp;</span>
	</div>
	<div class="accordion-fold subrow-container test_groups">
	<!-- hidden for now -->
		<div class="row hint">Assigned {{= assign_count }} time{{ if (assign_count != 1){ }}s{{ } }}:</div>
		<ul class="groups_links_list hover-row">
			{{ _.each(groups, function(group) { }}
			<li class="row">
				<div class="col-span-6">
					<p class="group-name name"><a href="/a/users/group/?rg_id={{= group['id'] }}" class="graylink">{{= group['name'] }}</a></p>
				</div>
				<div class="col-span-1 icon-calendar{{ if (group['available'] == 1) { }}-green{{ } }}">
					<span class="showTooltipHTML" title="{{= group['display_dates'] }}">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
				</div>
				<div class="col-span-3 col-btns littledark">
					<a class="" href="/a/assign/rgmanage/?show=1&amp;test_id={{= id }}&amp;rg_id={{= group['id'] }}">Settings</a>
					| <a href="/test/start/?test_id={{= id }}&amp;rg_id={{= group['id'] }}" class="showTooltip" title="Preview taking this test with these applied settings.">Preview</a>					<a href="/a/results/tests/test/rgusers/?test_id={{= id }}&amp;rg_id={{= group['id'] }}&amp;return=t" {{ if (group['results'] == 0) { }}class="btn btn-noresults "{{ } else {  }}class="btn btn-results "{{ } }}>Results</a>
				</div>
			</li>
			{{ }); }}
			{{ _.each(links, function(link) { }}
			<li class="row">
				<div class="col-span-6">
					<p class="link-name name"><a href="/a/assign/nrgmanage/?show=1&amp;test_id={{= id }}&amp;nrg_id={{= link['id'] }}" class="graylink">{{= link['name'] }}</a></p>
										<div class="linksinafold show-direct-links editor clear hide" id="{{= link['ext_test_id'] }}"><p><strong>Options for giving access to this Test:</p></strong>
						<p>Test link <span class="showTooltip tooltip" title="Use this format to send this link to your test takers via email. You can also post this link on your website, add to facebook, twitter etc.">tip</span>
							<br /><input class="textinput" type="text" size="60" value="https://www.classmarker.com/online-test/start/?quiz={{= link['ext_test_id'] }}"  onclick="javascript:this.focus();this.select();" readonly="readonly"/></p>

						<p>Embed test into your website <span class="showTooltipHTML tooltip" title="Insert this iFrame HTML code to embed this test directly in your website.<br/>Only use \'one\' test per \'web page\'.">tip</span>
							<br /><input class="textinput" type="text" size="60" value="&lt;iframe src=&quot;https://www.classmarker.com/online-test/start/?quiz={{= link['ext_test_id'] }}&amp;iframe=1&quot; frameborder=&quot;0&quot; style=&quot;width:100%;max-width:700px;&quot; height=&quot;800&quot;&gt;&lt;/iframe&gt;" onclick="javascript:this.focus();this.select();" readonly="readonly"/></p>
					</div>
									</div>
				<div class="col-span-1 icon-calendar{{ if (link['available'] == 1) { }}-green{{ } }}">
					<span class="showTooltipHTML" title="{{= link['display_dates'] }}">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
				</div>
				<div class="col-span-3 col-btns littledark">
					<a href="#" class="open_embed_links" id="{{= link['ext_test_id'] }}" data-ext_test_id="{{= link['ext_test_id'] }}">Links</a> | 				<a class="" href="/a/assign/nrgmanage/?show=1&amp;test_id={{= id }}&amp;nrg_id={{= link['id'] }}">Settings</a>
					| <a href="https://www.classmarker.com/online-test/start/?quiz={{= link['ext_test_id'] }}&amp;nrg_id={{= link['id'] }}" class="showTooltip" title="Preview taking this test with these applied settings.">Preview</a>					<a href="/a/results/tests/test/nrgusers/?test_id={{= id }}&amp;nrg_id={{= link['id'] }}&amp;return=t&amp;year=2020" {{ if (link['results'] == 0) { }}class="btn btn-noresults"{{ } else {  }}class="btn btn-results"{{ } }}>Results</a>
				</div>
			</li>
			{{ }); }}
		</ul>
		<div class="row assign">
			<a href="/a/assign/groups/?test_id={{= id }}" class="btn btn-assign-test">Assign</a>
		</div>
	</div>
</script>