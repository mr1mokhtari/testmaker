				<input class="sub-checkbox" type="checkbox" name="settings[show_correct_answers]" value="0" id="sca2_" <%if(edits=='0'){%>checked=true<%}else if(edits=='1'){if(setting.show_correct_answers=='0' && setting.show_answers=='0'){%> checked=true <%}}%>>
